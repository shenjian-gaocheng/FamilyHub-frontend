<view class="page">
  <view class="section">
    <view class="section-header">
      <text class="title">上传体检报告</text>
      <button class="primary" size="mini" bindtap="onUpload">上传文件</button>
    </view>
    <text class="note">支持电子报告，解析关键指标并写入档案。</text>
  </view>

  <view class="section">
    <text class="title">历史报告</text>
    <block wx:for="{{uploads}}" wx:key="id">
      <view class="card" bindtap="onView" data-id="{{item.id}}">
        <view class="card-top">
          <text class="card-name">{{item.name}}</text>
          <text class="status">{{item.status}}</text>
        </view>
        <text class="note">{{item.notes}}</text>
      </view>
    </block>
  </view>
</view>
